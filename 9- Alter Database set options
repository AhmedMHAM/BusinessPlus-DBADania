ALTER DATABASE Dania set read_only;
ALTER DATABASE Dania set READ_WRITE;
ALTER DATABASE Dania SET SINGLE_USER  WITH ROLLBACK IMMEDIATE;
ALTER DATABASE Dania SET MULTI_USER;  
ALTER DATABASE Dania SET AUTO_CLOSE ON; 
ALTER DATABASE Dania SET AUTO_CREATE_STATISTICS ON ; 
ALTER DATABASE Dania SET  AUTO_SHRINK ON;  
ALTER DATABASE Dania SET AUTO_UPDATE_STATISTICS ON;
ALTER DATABASE Dania SET AUTO_UPDATE_STATISTICS_ASYNC ON;

ALTER DATABASE Dania SET ENABLE_BROKER  
ALTER DATABASE Dania SET DISABLE_BROKER  
ALTER DATABASE Dania SET NEW_BROKER  
ALTER DATABASE Dania SET ERROR_BROKER_CONVERSATIONS  
ALTER DATABASE Dania SET HONOR_BROKER_PRIORITY ON;
  

ALTER DATABASE Dania SET ENCRYPTION ON;
ALTER DATABASE Dania SET ENCRYPTION OFF;
 
