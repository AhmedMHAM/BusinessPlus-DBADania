

use Dania

DBCC CHECKDB;  
DBCC CHECKDB (Dania, NOINDEX);   
DBCC CHECKDB WITH NO_INFOMSGS;    

DBCC CHECKALLOC; 
DBCC CHECKALLOC (Dania);  

DBCC CHECKCATALOG;
DBCC CHECKCATALOG (Dania); 

DBCC CHECKCONSTRAINTS(tbl_Dania); 

DBCC CHECKFILEGROUP;

Declare @var int;SELECT @var=@@spid; DBCC INPUTBUFFER (@var);
